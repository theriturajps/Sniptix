{
	"HTML5 Boilerplate": {
		"prefix": "html5",
		"body": [
			"<!DOCTYPE html>",
			"<html lang=\"${1:en}\">",
			"<head>",
			"    <meta charset=\"${2:UTF-8}\">",
			"    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
			"    <link rel=\"stylesheet\" href=\"${3:path/to/style.css}\" />",
			"    <title>${4:HTML5 Boilerplate}</title>",
			"</head>",
			"<body>",
			"    ${5:<!-- Your content goes here -->}",
			"<script src=\"${6:path/to/script.js}\"></script>",
			"</body>",
			"</html>"
		],
		"description": "HTML5 basic boilerplate"
	},
	"Anchor Tag": {
		"prefix": "a",
		"body": [
			"<a href=\"${1:#}\">${2:Link Text}</a>"
		],
		"description": "Code snippet for anchor tag"
	},
	"Image Tag": {
		"prefix": "img",
		"body": [
			"<img src=\"${1:path/to/image.jpg}\" alt=\"${2:description}\" />"
		],
		"description": "Code snippet for image tag"
	},
	"Div Element": {
		"prefix": "div",
		"body": [
			"<div>${1:content}</div>"
		],
		"description": "Code snippet for div tag"
	},
	"Script Tag": {
		"prefix": "script",
		"body": [
			"<script src=\"${1:path/to/script.js}\"></script>"
		],
		"description": "Code snippet for script tag"
	},
	"Link Tag (Stylesheet)": {
		"prefix": "link",
		"body": [
			"<link rel=\"stylesheet\" href=\"${1:path/to/style.css}\" />"
		],
		"description": "Code snippet for stylesheet link tag"
	},
	"Meta Charset Tag": {
		"prefix": "meta-charset",
		"body": [
			"<meta charset=\"${1:UTF-8}\">"
		],
		"description": "Code snippet for meta charset tag"
	},
	"Meta Viewport Tag": {
		"prefix": "meta-viewport",
		"body": [
			"<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">"
		],
		"description": "Code snippet for meta viewport tag"
	},
	"Header Tag": {
		"prefix": "header",
